<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.moumou.bootmybatisdemo.dataAlignment.mapper.UserMapper">
    <select id="userQueryByName" resultType="com.moumou.bootmybatisdemo.dataAlignment.model.User" >
        select
        id,
        password,
        username
        from
        user
        where username = #{username}
    </select>



</mapper>